// WRITTEN BY CHRIS DILLOW | NOVEMBER 30, 2025 | TROPOCHIEF RC PLANE PROJECT
// FIELD: Fluid Density Rho
FoamFile
{
    version 2.0;
    format ascii;
    class volScalarField;
    location "0";
    object rho;
}

dimensions [1 -3 0 0 0 0 0];

internalField uniform 1.225;

boundaryField
{
    inlet
    {
        type fixedValue;
        value uniform 1.225;
    }

    outlet
    {
        type zeroGradient;
    }

    airfoil
    {
        type zeroGradient;
    }

    top
    {
        type zeroGradient;
    }

    bottom
    {
        type zeroGradient;
    }

    frontAndBack
    {
        type empty;
    }
}